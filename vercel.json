{
    "devCommand": "uvicorn app.main:app",
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.12" }
      }
    ],
    "routes": [
      {
        "src": "/(static/.*)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "app/main.py"
      }
    ],
    "installCommand": "pip install --no-cache-dir --upgrade -r requirements.txt"
  }